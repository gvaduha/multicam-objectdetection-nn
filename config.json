{
  "nn": {
    "module": "torchdetector",
    "class": "TorchDetector"
  },
  "resultsink": {
    "module": "webservice",
    "class": "FlaskResultSink"
  },
  "runintervalsec": 1.0,
  "modules": {
    "WriteJsonResultSink": {
      "file": "./results.log"
    },
    "FlaskResultSink": {
      "server": "0.0.0.0:5555",
      "resultep": "/currentresult"
    },
    "TensorFlowDetector": {
      "model": "models/tf.model",
      "threshold": 0.1
    },
    "TorchDetector": {
      "model": "models/torch.model",
      "threshold": 0.1,
      "device": "cuda"
    }
  },
  "cams": [
    {
      "vsid": 1,
      "uri": "rtsp://admin:admin@cam1/h264"
    },
    {
      "vsid": 2,
      "uri": "rtsp://admin:admin@cam2"
    }
  ]
}
